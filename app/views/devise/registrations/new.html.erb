<% if params[:role] == '1' %>
  <h1>Basic Account</h1>
<% else %>
  <h1>Pro Account</h1>
<% end %>
<div class=row>
    <div class="col-md-4 col-md-offset-4">
        <div class="well">
            <div class="form-group">
                <h2>Sign up</h2>
                
                <% if params[:role] == '2' %>
                  <%= render "teacher_form" %>
                <% else  %>
                  <%= render "student_form" %>
                <% end  %>
<%= render "devise/shared/links" %>
        </div>
    </div>
</div>